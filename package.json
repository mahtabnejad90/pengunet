{
  "name": "pengunet",
  "version": "1.0.2",
  "description": "CLI to log browser network requests (response, headers, initiator, timing) via Playwright + CDP",
  "bin": {
    "pengunet": "dist/pengunet.js"
  },
  "type": "module",
  "scripts": {
    "build": "./node_modules/.bin/tsc",
    "start": "tsx src/pengunet.ts",
    "prepublishOnly": "npm run build"
  },
  "ts-node": {
    "esm": true
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "playwright": "^1.55.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/yargs": "^17.0.0",
    "ts-node": "^10.9.0",
    "tsx": "^4.20.5",
    "typescript": "^5.0.0"
  },
  "main": "dist/pengunet.js",
  "keywords": [
    "cli",
    "network",
    "monitoring",
    "playwright",
    "browser",
    "penguin"
  ],
  "author": "Mahtab Nejad",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/mahtabnejad90/pengunet.git"
  },
  "bugs": {
    "url": "https://github.com/mahtabnejad90/pengunet/issues"
  },
  "homepage": "https://github.com/mahtabnejad90/pengunet#readme",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ]
}
